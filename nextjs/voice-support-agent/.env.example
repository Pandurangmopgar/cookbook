# MemoryStack API Key (get from memorystack.app/dashboard/api-keys)
MEMORYSTACK_API_KEY=mem_live_xxx

# Vapi Configuration (get from dashboard.vapi.ai)
# Your Vapi API key
VAPI_API_KEY=your-vapi-api-key
# Phone number ID from Vapi dashboard (after purchasing a number)
VAPI_PHONE_NUMBER_ID=
# Optional: Pre-created assistant ID (will create dynamically if not set)
VAPI_ASSISTANT_ID=
# Optional: Webhook secret for signature verification
VAPI_SERVER_SECRET=

# Your app's public URL (use ngrok for local dev)
# Example: https://abc123.ngrok.io
NEXT_PUBLIC_APP_URL=https://your-ngrok-url.ngrok.io

# Upstash Redis for real-time call tracking (get from console.upstash.com)
UPSTASH_REDIS_URL=https://your-redis.upstash.io
UPSTASH_REDIS_TOKEN=your-redis-token

# Optional: Google API Key (if using Gemini directly for other features)
# GOOGLE_API_KEY=
